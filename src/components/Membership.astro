<section
  id="membership"
  class="region wrapper flow prose text-center"
  aria-labelledby="membership-pricing"
>
  <h2 id="membership-pricing" class="mx-auto">Membership options</h2>
  <div class="grid text-center" data-layout="thirds">
    <div
      class="flex flex-col flow bg-transparent box rounded-3xl border-solid border-1 border-gray-600"
    >
      <h3>Monthly</h3>
      <p>
        Simple terms, few clients, maximum focus. 1 out of 2 slots available.
      </p>
      <h3>
        $10,000<small class="text-step-00 font-bold text-gray-1000"
          >/month</small
        >
      </h3>
      <p>
        <small class="text-step-00">Pause or cancel anytime</small>
      </p>
      <p class="self-center">
        <a href="https://buy.stripe.com/cN27sM7Ym9Vr1ji9AH" class="button"
          >Get started</a
        >
      </p>
    </div>
    <div
      class="flex flex-col flow bg-transparent box rounded-3xl border-solid border-1 border-gray-600"
    >
      <h3>Weekly</h3>
      <p>
        Simple terms, few clients, maximum focus. 1 out of 2 slots available.
      </p>
      <h3>
        $2,600<small class="text-step-00 font-bold text-gray-1000">/week</small>
      </h3>
      <p>
        <small class="text-step-00">Cancel anytime</small>
      </p>
      <p class="self-center">
        <a href="https://buy.stripe.com/14k5kEceCffL3rq28e" class="button"
          >Get started</a
        >
      </p>
    </div>
    <div
      class="flex flex-col flow bg-transparent justify-between box rounded-3xl border-solid border-1 border-gray-600"
    >
      <div class="flex flex-col flow">
        <h3>Book enrollment call</h3>
        <p>Learn more about designzen, and how it can help you.</p>
      </div>
      <p class="self-center">
        <a href="https://tidycal.com/frankstallone/designzen" class="button"
          >Book a call</a
        >
      </p>
    </div>
  </div>
</section>

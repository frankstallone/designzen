---
import Layout from '../layouts/Layout.astro';
import Hero from '../components/Hero.astro';
import WorkedWith from '../components/WorkedWith.astro';
import Process from '../components/Process.astro';
import Outcomes from '../components/Outcomes.astro';
import Benefits from '../components/Benefits.astro';
import Membership from '../components/Membership.astro';
import FAQ from '../components/FAQ.astro';
import Footer from '../components/Footer.astro';
---

<Layout>
  <main role="main" id="main">
    <Hero />
    <WorkedWith />
    <Process />
    <Outcomes />
    <Benefits />
    <div
      class="bg-gradient-to-bl from-indigo-100 to-blue-100 text-center relative z-10 text-indigo-1000"
    >
      <Membership />
      <FAQ />
    </div>
    <Footer />
  </main>
</Layout>
